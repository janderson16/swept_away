<table class="table transparent_div table-hover table-striped" id="routes">
    <thead>
      <tr>
        <th>Map</th>
        <th>Street</th>
        <th>From</th>
        <th>To</th>
        <th>Week of month</th>
        <th>Days of week</th>
        <th>Start</th>
        <th>Finish</th>
        <th>Side of street</th>
      </tr>
    </thead>
    <div class="routes">
      <% @routes.each do |route| %>
        <tbody class="page">
          <div data="<%= route.street %>">
            <tr class="post route-row route route<%= route.mainid %>">
              <td class="route-name"><p> <%= link_to "Show Map", "/routes/#{route.mainid}", mainid: route.mainid%></p></td>
              <td><p><%= route.street %></p></td>
              <td><p><%= route.start_point %></p></td>
              <td><p><%= route.end_point %></p></td>
              <td><p><%= route.weeks_of_month %></p></td>
              <td><p data-day="<%= route.days_of_week %>"><%= route.days_of_week %> </p></td>
              <td><p><%= route.start_time.slice(0..4) %></p></td>
              <td><p><%= route.end_time.slice(0..4) %></p></td>
              <td><p><%= route.side %></p></td>
            </tr>
          </div>
        </tbody>
      <% end %>
    </div>
</table>
